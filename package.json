{
  "name": "@masatomakino/gulptask-demo-page",
  "version": "0.10.5",
  "description": "demo html generator for gulp.js",
  "main": "bin/index.js",
  "type": "module",
  "types": "bin/index.d.ts",
  "bin": {
    "gulptask-demo-page": "bin/CLI.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/gulptask-demo-page.git"
  },
  "files": [
    "bin",
    "template",
    "webpack.config.js",
    "tsconfig.page.json"
  ],
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^5.0.0",
    "commander": "^14.0.0",
    "ejs": "^4.0.1",
    "glob": "13.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.1.3",
    "webpack": "^5.102.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/ejs": "^3.1.2",
    "@types/node": "^25.0.3",
    "@vitest/coverage-istanbul": "^4.0.4",
    "webpack-glsl-loader": "^1.0.1"
  },
  "scripts": {
    "test": "vitest --run && npm run build && npm run testRun",
