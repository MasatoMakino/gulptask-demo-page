  "description": "demo html generator for gulp.js",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "bin": {
    "gulptask-demo-page": "bin/CLI.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/gulptask-demo-page.git"
  },
  "files": [
    "bin",
    "template",
    "webpack.config.js",
    "tsconfig.page.json"
  ],
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "ejs": "^3.1.8",
    "glob": "^8.0.3",
    "recursive-copy": "^2.0.14",
    "source-map-loader": "^2.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/node": "^17.0.36",
    "moment": "^2.29.3",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "webpack-glsl-loader": "^1.0.1"
  },
  "scripts": {
    "build": "npx tsc --project ./tsconfig.json",
    "watch": "npx tsc -W --project ./tsconfig.json",
    "exampleCLI": "npx @masatomakino/gulptask-demo-page --prefix demo --srcDir ./demoSrc --distDir ./docs/demo --body '<div></div>' --style canvas{background-color:#000} --externalScripts 'https://code.createjs.com/1.0.0/createjs.min.js' --rule ./rule.js",
    "watch:exampleCLI": "npm run exampleCLI -- -W",
    "testRun": "node ./bin/CLI.js --srcDir ./demoSrc",
    "watch:testRun": "npm run testRun -- -W"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/gulptask-demo-page/issues"
  },
  "homepage": "https://github.com/MasatoMakino/gulptask-demo-page#readme",
  "directories": {
    "doc": "docs"
  }
}
